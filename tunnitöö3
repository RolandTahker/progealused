// for
// while


for (let number=1; number <= 100; number++) {
    console.log(number)
} 


-------------------------------------------------------------------

for (let index=1; number <= 100; number++) {
    console.log(i)
} 


-------------------------------------------------------------------



for (let =1; number <= 100; number++) {
    console.log(number)
}   

-------------------------------------------------------------------


for (let x=1; x <= 100; x++) {
    console.log(x)
}

-------------------------------------------------------------------


let x=1
while(x <= 100) {
    console.log(x);
    x++;
}

